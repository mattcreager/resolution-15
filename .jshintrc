{
    "maxerr"        : 50,       // {int} Maximum error before stopping

    // Enforcing
    "camelcase"     : true,    // true: Identifiers must be in camelCase
    "curly"         : false,   // true: Require {} for every new block or scope
    "indent"        : 2,       // {int} Number of spaces to use for indentation
    "newcap"        : true,    // true: Require capitalization of all constructor functions
    "quotmark"      : true,

    "maxdepth"      : 5,

    // Environments
    "browser"       : false,
    "browserify"    : true,
    "couch"         : false,
    "devel"         : true,
    "dojo"          : false,
    "jasmine"       : false,
    "jquery"        : false,
    "mocha"         : true,
    "mootools"      : false,
    "node"          : false,
    "nonstandard"   : false,
    "prototypejs"   : false,
    "qunit"         : false,
    "rhino"         : false,
    "shelljs"       : false,
    "worker"        : false,
    "wsh"           : false,
    "yui"           : false,

    // Custom Globals
    "globals"       : {}
}